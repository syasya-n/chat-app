<div class="side-header">
  <div class="user-name">
    <%= link_to current_user.name,edit_user_path(current_user)%>
  </div>
  <div class="chat-create-btn">
  <a href="#">チャットを作成する</a>
  </div>
</div>
<div class="side-chatrooms">
  <% current_user.rooms.each do |room|%>
    <div class="room">
      <div class="room-name">
        <a href="#"><%=room.name%></a>
      </div>
    </div>
  <% end %>
</div>
